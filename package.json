{
	"name": "my-first-extension",
	"displayName": "My first extension",
	"description": "Learning vscode extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.95.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "my-first-extension.searchDotCodeProjects",
				"title": "DotCode: Search Projects"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dotCodeSidebar",
					"title": "DotCode",
					"icon": "resources/dot.svg"
				}
			]
		},
		"views": {
			"dotCodeSidebar": [
				{
					"id": "dotCodeView",
					"name": "DotCode",
					"icon": "resources/dot.svg",
					"contextualTitle": "DotCode"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.9",
		"@types/node": "20.x",
		"@types/vscode": "^1.95.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^9.13.0"
	},
	"dependencies": {
		"axios": "^1.7.7"
	}
}
